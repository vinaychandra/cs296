set auto x
set offset 0,0,.5,.5
set autoscale fix
set boxwidth 0.3
set style fill solid

plot "./data/average.dat" u 2:4
min_error = GPVAL_DATA_Y_MIN
plot "./data/average.dat" u 2:4
max_error = GPVAL_DATA_Y_MAX

bw = (max_error-min_error)/10

set boxwidth bw*0.7
 
bin_number(x) = floor(x/bw)

rounded(x) = bw * ( bin_number(x) + 0.5 )

set term png
set output "./plots/g31_plot04.png"

plot "./data/average.dat" using (rounded($4)):(1) smooth frequency with  boxes,"./data/average.dat" using (rounded($4)):(1) smooth cumulative
